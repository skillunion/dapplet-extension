{
    "manifest_version": 2,
    "name": "Dapplet Extension",
    "version": "0.0.1",
    "description": "Adds a dapplet button to Legacy web-sites.",
    "background": {
        "page": "background.html"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"], 
            "js": [
                "extension.js"
            ],
            "run_at": "document_end"
        }
    ]
}